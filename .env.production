# Production Environment Variables
NODE_ENV=production

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@wangov-db:5432/wangovid

# JWT Secrets (CHANGE THESE IN PRODUCTION!)
JWT_SECRET=your-super-secret-jwt-key-change-in-production-min-32-chars
ADMIN_JWT_SECRET=your-super-secret-admin-jwt-key-change-in-production-min-32-chars
SESSION_SECRET=your-super-secret-session-key-change-in-production-min-32-chars

# Database Credentials
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=wangovid

# Redis Configuration
REDIS_URL=redis://wangov-redis:6379

# API Configuration
API_PORT=3000
API_HOST=0.0.0.0

# Frontend Configuration
REACT_APP_API_URL=http://localhost:3000/api

# Security
CORS_ORIGIN=http://localhost:3001,http://localhost:3002,http://localhost:3003,http://localhost:3004

# Logging
LOG_LEVEL=info
